<!-- Bieu do 1 -->
<script>
    Highcharts.chart('powerHour87', {
          chart: {
            backgroundColor:'rgba(255, 255, 255, 0.9)',
              type: 'line',
              zoomType: 'x'
          },
          title: {
              text: 'LVMSB1 - <%= title %>',
              style: {
                  color: "#fff"
               }
          },
          xAxis: {
            categories: [
            <% for(let i=0; i<power87.length; i+=30){ %>
            '<%= power87[i].Time %>',
            <% } %>
            ]
          },
          yAxis: {
              title: {
                  text: 'kWh'
              },
            
          },
          series: [{
              name: 'LVMSB1',
              data: [
                <% for(let i=0; i<power87.length; i+=30){ %>
                    <%= power87[i].powerH %>,
                <% } %>
                ],
                color:'#66ccff'
                }],
      });
</script>

